<h5><%= link_to 'Edit', edit_artist_path(@artist) %>
<%= link_to 'Delete', @artist, method: :delete %></h5>


<h1><%= @artist.name %></h1>
<h2>Genre - <%= @artist.genre %></h2>

<p><% @artist.songs.each do |song| %>
  <h2><%= link_to song.name, [@artist, song] %></h2>
  <h3><%= link_to "Play", song.youtubelink %>
  <%= link_to 'Edit', edit_artist_song_path(@artist, song) %>
 
  <%= link_to 'Delete', [@artist, song], method: :delete %></h3>
   <% end %></p>
   <hr>
   <%= link_to 'New Song', new_artist_song_path(@artist) %>


<p><%= link_to 'Back', artists_path %></p>

